{
  "manifest_version": 3,
  "name": "CRM Extension",
  "version": "1.0",
  "description": "Uma extens√£o para CRM",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
